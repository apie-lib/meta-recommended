{
  "name": "apie/meta-recommended",
  "description": "Composer package of the apie library: meta recommended",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\MetaRecommended\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\MetaRecommended\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "apie/core": "self.version",
    "apie/maker": "self.version",
    "apie/meta-minimal": "self.version",
    "apie/common-value-objects": "self.version",
    "apie/country-and-phone-number": "self.version",
    "apie/date-value-objects": "self.version",
    "apie/doctrine-entity-datalayer": "self.version",
    "apie/faker": "self.version",
    "apie/text-value-objects": "self.version"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}